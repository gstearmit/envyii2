{
    "name": "gstearmit/envyii2",
    "description": "Loads environment variables from `.env` to `getenv()`, `$_ENV` and `$_SERVER` automagically.",
    "keywords": ["env", "dotenv", "environment"],
    "homepage": "https://github.com/gstearmit/envyii2.git",
    "license": "MIT",
    "authors": [
        {
            "name": "gstearmit",
            "email": "gstearmit@gmail.com"
        }
    ],
    "minimum-stability": "stable",


    "require": {
        "php": ">=5.3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Dotenv\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
